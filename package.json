{
  "name": "mermaid-gdocs",
  "version": "1.0.0",
  "description": "Mermaidsjs wrapper for gdocs",
  "private": true,
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "start": "webpack-dev-server --content-base ./dist",
    "build": "webpack",
    "pretty": "prettier --single-quote=true --no-bracket-spacing --jsx-bracket-same-line --trailing-comma=es5 --write src/**/*.{js,css}",
    "size": "SHOW_BUNDLE=true npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/renanlecaro/mermaid-gdocs.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/renanlecaro/mermaid-gdocs/issues"
  },
  "homepage": "https://github.com/renanlecaro/mermaid-gdocs#readme",
  "devDependencies": {
    "css-loader": "^2.1.1",
    "html-webpack-inline-source-plugin": "0.0.10",
    "html-webpack-plugin": "^3.2.0",
    "prettier": "^1.19.1",
    "style-loader": "^0.23.1",
    "webpack": "^4.41.5",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  },
  "dependencies": {}
}
